{
  "name": "@lrnwebcomponents/lrn-shared-styles",
  "wcfactory": {
    "className": "LrnSharedStyles",
    "customElementClass": "PolymerElement",
    "elementName": "lrn-shared-styles",
    "generator-wcfactory-version": "0.4.0",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/lrn-shared-styles.css",
      "scss": "src/lrn-shared-styles.scss",
      "html": "src/lrn-shared-styles.html",
      "js": "src/lrn-shared-styles.js",
      "properties": "src/lrn-shared-styles-properties.json",
      "hax": "src/lrn-shared-styles-hax.json"
    }
  },
  "version": "2.1.0",
  "description": "a shared set of styles for all @lrnwebcomponents",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "lrn-shared-styles.js",
  "module": "lrn-shared-styles.es6.js",
  "umd": "lrn-shared-styles.umd.js",
  "amd": "lrn-shared-styles.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/lrn-shared-styles/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/polymer": "^3.2.0"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@webcomponents/webcomponentsjs": "2.2.10",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.11",
    "polymer-cli": "1.9.10",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ],
  "gitHead": "e38751e6f76a812e00ae87ef826fafcdbfec4083"
}
