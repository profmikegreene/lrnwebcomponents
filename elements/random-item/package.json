{
  "name": "@lrnwebcomponents/random-item",
  "wcfactory": {
    "className": "RandomItem",
    "customElementClass": "HTMLElement",
    "elementName": "random-item",
    "generator-wcfactory-version": "0.7.3",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/random-item.css",
      "scss": "src/random-item.scss",
      "html": "src/random-item.html",
      "js": "src/random-item.js",
      "properties": "src/random-item-properties.json",
      "hax": "src/random-item-hax.json"
    }
  },
  "version": "2.1.0",
  "description": "show a random item from a list of items",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "random-item.js",
  "module": "random-item.es6.js",
  "umd": "random-item.umd.js",
  "amd": "random-item.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/random-item/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/hax-body-behaviors": "latest"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.0.9",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.2.10",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.11",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.10",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
