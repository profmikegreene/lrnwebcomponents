{
  "name": "@lrnwebcomponents/fluid-type",
  "wcfactory": {
    "className": "FluidType",
    "customElementClass": "HTMLElement",
    "elementName": "fluid-type",
    "generator-wcfactory-version": "0.7.3",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/fluid-type.css",
      "scss": "src/fluid-type.scss",
      "html": "src/fluid-type.html",
      "js": "src/fluid-type.js",
      "properties": "src/fluid-type-properties.json",
      "hax": "src/fluid-type-hax.json"
    }
  },
  "version": "2.1.0",
  "description": "A simple fluid-type sizing wrapper element to apply to anything",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "fluid-type.js",
  "module": "fluid-type.es6.js",
  "umd": "fluid-type.umd.js",
  "amd": "fluid-type.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/fluid-type/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "0.2.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@webcomponents/webcomponentsjs": "2.2.10",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.11",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.10",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
