{
  "name": "@lrnwebcomponents/agency-theme",
  "wcfactory": {
    "className": "AgencyTheme",
    "customElementClass": "PolymerElement",
    "elementName": "agency-theme",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/agency-theme.css",
      "scss": "src/agency-theme.scss",
      "html": "src/agency-theme.html",
      "js": "src/agency-theme.js",
      "properties": "src/agency-theme-properties.json",
      "hax": "src/agency-theme-hax.json"
    }
  },
  "version": "2.1.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "agency-theme.js",
  "module": "agency-theme.es6.js",
  "umd": "agency-theme.umd.js",
  "amd": "agency-theme.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/agency-theme/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "open ./src && concurrently --kill-others 'yarn run watch' 'yarn run serve'",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "heymp"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@polymer/polymer": "^3.2.0"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@webcomponents/webcomponentsjs": "2.2.10",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.11",
    "polymer-cli": "1.9.10",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
