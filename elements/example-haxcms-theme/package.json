{
  "name": "@lrnwebcomponents/example-haxcms-theme",
  "wcfactory": {
    "className": "ExampleHaxcmsTheme",
    "customElementClass": "PolymerElement",
    "elementName": "example-haxcms-theme",
    "generator-wcfactory-version": "0.7.3",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/example-haxcms-theme.css",
      "scss": "src/example-haxcms-theme.scss",
      "html": "src/example-haxcms-theme.html",
      "js": "src/example-haxcms-theme.js",
      "properties": "src/example-haxcms-theme-properties.json",
      "hax": "src/example-haxcms-theme-hax.json"
    }
  },
  "version": "2.1.0",
  "description": "A basic, well documented example theme for HAXcms",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "example-haxcms-theme.js",
  "module": "example-haxcms-theme.es6.js",
  "umd": "example-haxcms-theme.umd.js",
  "amd": "example-haxcms-theme.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/example-haxcms-theme/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/haxcms-elements": "^2.1.0",
    "@lrnwebcomponents/simple-colors": "^2.1.0",
    "@polymer/polymer": "^3.0.5"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.0.9",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@webcomponents/webcomponentsjs": "2.2.10",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.11",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.10",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
